      <li>GRGlobal</li>
    </include>
  <rulePack>
    <rulesStrings>
      <li>sent->[INITIATOR_pronoun] [harassed] [RECIPIENT_pronoun]'s [crudefemalebodypart].</li>
      <li>harrassed->pinched</li>
      <li>harrassed->stroked</li>
      <li>harrassed->slapped</li>
      <li>harrassed->groped</li>

    </rulesStrings>
  </rulePack>
</RulePackDef>

